{
    "name":  "infernix-executor",
    "private":  true,
    "version":  "1.1.9",
    "description":  "The next-generation Roblox executor",
    "author":  "Infernix Team",
    "main":  "electron/main.cjs",
    "type":  "module",
    "scripts":  {
                    "dev":  "vite",
                    "build":  "vite build",
                    "electron":  "electron .",
                    "electron:dev":  "concurrently \"npm run dev\" \"wait-on tcp:5173 -t 30000 \u0026\u0026 electron . || wait-on tcp:5174 -t 30000 \u0026\u0026 electron .\"",
                    "lint":  "eslint .",
                    "preview":  "vite preview",
                    "pack":  "npm run build \u0026\u0026 electron-builder --dir",
                    "dist":  "npm run build \u0026\u0026 electron-builder",
                    "dist:win":  "npm run build \u0026\u0026 electron-builder --win"
                },
    "dependencies":  {
                         "@monaco-editor/react":  "^4.7.0",
                         "framer-motion":  "^12.29.2",
                         "lucide-react":  "^0.563.0",
                         "react":  "^19.2.0",
                         "react-dom":  "^19.2.0"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.39.1",
                            "@types/react":  "^19.2.5",
                            "@types/react-dom":  "^19.2.3",
                            "@vitejs/plugin-react":  "^5.1.1",
                            "concurrently":  "^9.2.1",
                            "electron":  "^40.1.0",
                            "electron-builder":  "^26.7.0",
                            "eslint":  "^9.39.1",
                            "eslint-plugin-react-hooks":  "^7.0.1",
                            "eslint-plugin-react-refresh":  "^0.4.24",
                            "globals":  "^16.5.0",
                            "vite":  "npm:rolldown-vite@7.2.5",
                            "wait-on":  "^9.0.3"
                        },
    "overrides":  {
                      "vite":  "npm:rolldown-vite@7.2.5"
                  },
    "build":  {
                  "appId":  "com.infernix.executor",
                  "productName":  "Infernix",
                  "directories":  {
                                      "output":  "release"
                                  },
                  "files":  [
                                "dist/**/*",
                                "electron/**/*",
                                "bin/**/*"
                            ],
                  "extraResources":  [
                                         {
                                             "from":  "bin",
                                             "to":  "bin",
                                             "filter":  [
                                                            "**/*"
                                                        ]
                                         }
                                     ],
                  "win":  {
                              "target":  [
                                             {
                                                 "target":  "nsis",
                                                 "arch":  [
                                                              "x64"
                                                          ]
                                             }
                                         ],
                              "icon":  "build/icon.ico"
                          },
                  "nsis":  {
                               "oneClick":  true,
                               "perMachine":  false,
                               "allowToChangeInstallationDirectory":  false,
                               "installerIcon":  "build/icon.ico",
                               "uninstallerIcon":  "build/icon.ico",
                               "installerHeaderIcon":  "build/icon.ico",
                               "createDesktopShortcut":  true,
                               "createStartMenuShortcut":  true,
                               "shortcutName":  "Infernix",
                               "include":  "build/installer.nsh",
                               "installerHeader":  "build/installerHeader.bmp",
                               "installerSidebar":  "build/installerSidebar.bmp",
                               "uninstallerSidebar":  "build/installerSidebar.bmp",
                               "license":  "build/license.txt",
                               "warningsAsErrors":  false
                           }
              }
}
